function userSignup()
{
 var new_form = new FormData($('#userSignUp')[0]);
 $.ajax({
    url: "/user/signup/<%= clinic_id %>",
    type: 'POST',
    data: new_form,
    async: true,
    success: function (data) {
      if(data.status==200) {
        alert("success");
        location.reload();
    }
    else
    {
      alert(data.message.info);
  }
},
cache: false,
contentType: false,
processData: false
});
}

//DOCTOR LOGIN

function userLogin()
{

    var email=$("#userEmail").val();
    var password=$("#userPassword").val();
    var data = {};
    data.email = email;
    data.password= password;
    $.ajax({
      type: 'POST',
      dataType : "json",
      contentType: "application/json; charset=utf-8",
      data: JSON.stringify(data),
      url:'/user/login',
      success: function(data) {
        if(data.status==200)
        {
            console.log(data);
            window.location.replace('/user/dashboard');
        }
        else
        {

          alert(data.message.info);

      }


  }


});

}



