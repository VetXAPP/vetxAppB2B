<script>
$(document).ready(function() {
        // Add scrollspy to <body>
        $('body').scrollspy({
            target: ".navbar",
            offset: 50
        });

        // Add smooth scrolling on all links inside the navbar
        $("#custom-nav a").on('click', function(event) {
            // Make sure this.hash has a value before overriding default behavior
            if (this.hash !== "") {
                // Prevent default anchor click behavior
                event.preventDefault();

                // Store hash
                var hash = this.hash;

                // Using jQuery's animate() method to add smooth page scroll
                // The optional number (800) specifies the number of milliseconds it takes to scroll to the specified area
                $('html, body').animate({
                    scrollTop: $(hash).offset().top
                }, 800, function() {

                    // Add hash (#) to URL when done scrolling (default click behavior)
                    window.location.hash = hash;
                });
            } // End if
        });
    });
</script>
<script>
function readURL(input) {
    if (input.files && input.files[0]) {
        var reader = new FileReader();

        reader.onload = function(e) {
            $('#blah').attr('src', e.target.result);
        }

        reader.readAsDataURL(input.files[0]);
    }
}

$("#imgInp").change(function() {
    readURL(this);
});
</script>

<script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.12.0/jquery.validate.min.js"></script>

<script type="text/javascript">



$(function() {
  // Initialize form validation on the registration form.
  // It has the name attribute "registration"
  $("form[name='user_login']").validate({
    // Specify validation rules
    rules: {

      email: {
        required: true,
        email: true
    },
    password: {
        required: true,
        minlength: 5
    }
},
    // Specify validation error messages
    messages: {

      password: {
        required: "Please provide a password *",
        minlength: "Your password must be at least 5 characters long *"
    },
    email: "Please enter a valid email address *"
},
    // Make sure the form is submitted to the destination defined
    // in the "action" attribute of the form when valid
    submitHandler: function(form) {

        userLogin();


    }
});
});

//USER SIGN

$(function() {
  // Initialize form validation on the registration form.
  // It has the name attribute "registration"
  $("form[name='userSignUp']").validate({
    // Specify validation rules
    rules: {
      firstName: {
        required: true
    },
    lastName: {
        required: true,

    },
    email: {
        required: true,
        minlength: 5
    },
    password: {
        required: true,
        minlength: 5
    }
},
    // Specify validation error messages
    messages: {

      password: {
        required: "Please provide a password *",
        minlength: "Your password must be at least 5 characters long *"
    },
    email: "Please enter a valid email address *",
    firstName: "Please provide firstname *",
    lastName: "Please provide lastname *"

},
    // Make sure the form is submitted to the destination defined
    // in the "action" attribute of the form when valid
    submitHandler: function(form) {
       userSignup();
   }
});
});

function userSignup()
{
 var new_form = new FormData($('#userSignUp')[0]);
 $.ajax({
    url: "/user/signup/<%= clinic_id %>",
    type: 'POST',
    data: new_form,
    async: true,
    success: function (data) {
      if(data.status==200) {
        alert("success");
        location.reload();
    }
    else
    {
      alert(data.message.info);
  }
},
cache: false,
contentType: false,
processData: false
});
}

//DOCTOR LOGIN

function userLogin()
{

    var email=$("input[name=email]").val();
    var password=$("input[name=password]").val();
    var data = {};
    data.email = email;
    data.password= password;
    $.ajax({
      type: 'POST',
      dataType : "json",
      contentType: "application/json; charset=utf-8",
      data: JSON.stringify(data),
      url:'/user/login',
      success: function(data) {
        if(data.status==200)
        {
            console.log(data);
            window.location.replace('/user/dashboard');
        }
        else
        {

          alert(data.message.info);

      }


  }


});

}



