function userSignup()
{
 var new_form = new FormData($('#userSignUp')[0]);
 $.ajax({
  url: "/user/signup/<%= clinic_id %>",
  type: 'POST',
  data: new_form,
  async: true,
  success: function (data) {
    if(data.status==200) {
      alert("success");
      location.reload();
    }
    else
    {
      alert(data.message.info);
    }
  },
  cache: false,
  contentType: false,
  processData: false
});
}

